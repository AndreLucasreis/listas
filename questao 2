int main(){
   
    cout << "DIgite dois valores inteiros\n";
    int numero1;
    int numero2;
    cin >> numero1; cin >> numero2;
    
    cout << numero1 + numero2 ;cout <<"\n"; 
	  cout << numero1 * numero2 ;cout <<"\n"; 
