#include <string>
#include <algorithm>
#include <bits/stdc++.h>	
#include <iostream>
using namespace std;

int main() {
	int i = 1;
	char resposta = '*';
	while (i < 6){
		int numeroestrela;
		cin >> numeroestrela;
		string estrela(numeroestrela, resposta);
		cout << estrela << endl;
		i++;
	}
}
	

