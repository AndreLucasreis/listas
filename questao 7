#include <string>
#include <algorithm>
#include <bits/stdc++.h>	
#include <iostream>
using namespace std;

int main() {
  string senha = "1234";
  string senhareal;
  cin >> senhareal;
  if (senhareal == senha){
  	cout << "acesso permitido" << endl;  	
  }
  else {
  	cout << "acesso negado" << endl;
  }
}
