#include <iostream>
using namespace std;

int main(){
   int a;
   int b;
   cin >> a; cin >> b;
   if (a % b == 0) {
        cout << a << " e multiplo de " << b << endl;
    } else {
        cout << a << " não e multiplo de " << b << endl;
    }

   
   
}
